<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="dh_ag95_gripper">

  <!-- Include transmission definitions -->
  <xacro:include filename="$(find dh_ag95_gripper)/urdf/dh_ag95_gripper.transmission.xacro"/>

  <!-- Gripper macro definition -->
  <xacro:macro name="dh_ag95_gripper" params="prefix parent *origin">
    
    <!-- Joint connecting gripper to parent -->
    <joint name="${prefix}gripper_base_joint" type="fixed">
      <parent link="${parent}"/>
      <child link="${prefix}gripper_base_link"/>
      <xacro:insert_block name="origin"/>
    </joint>

    <!-- Base link -->
    <link name="${prefix}gripper_base_link">
      <visual>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/base_link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/base_link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.83416"/>
        <origin xyz="0.0 0.0 0.0"/>
        <inertia ixx="0.0013016" ixy="-6.9374e-07" ixz="5.9848e-07"
                 iyy="0.00049972" iyz="2.0244E-07"
                 izz="0.001498"/>
      </inertial>
    </link>

    <!-- Finger 1 knuckle joint -->
    <joint name="${prefix}gripper_finger1_joint" type="revolute">
      <parent link="${prefix}gripper_base_link"/>
      <child link="${prefix}gripper_finger1_knuckle_link"/>
      <axis xyz="0 -1 0"/>
      <origin rpy="0.0 0.0 ${pi}" xyz="-0.016 0 0.10586"/>
      <limit lower="0.0" upper="0.65" velocity="2.0" effort="1000"/>
      <dynamics damping="0.1" friction="0.1"/>
    </joint>

    <!-- Finger 1 knuckle link -->
    <link name="${prefix}gripper_finger1_knuckle_link">
      <visual>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/crank_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/crank_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0222871"/>
        <origin xyz="0.0 0.0 0.0"/>
        <inertia ixx="8.72836e-06" ixy="7.0843e-15" ixz="-2.18833e-06"
                 iyy="7.26157e-06" iyz="7.6579e-15"
                 izz="3.3425e-06"/>
      </inertial>
    </link>

    <!-- Finger 2 knuckle joint -->
    <joint name="${prefix}gripper_finger2_joint" type="revolute">
      <parent link="${prefix}gripper_base_link"/>
      <child link="${prefix}gripper_finger2_knuckle_link"/>
      <axis xyz="0 -1 0"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.016 0 0.10586"/>
      <limit lower="0.0" upper="0.65" velocity="2.0" effort="1000"/>
      <dynamics damping="0.1" friction="0.1"/>
      <mimic joint="${prefix}gripper_finger1_joint" multiplier="1" offset="0"/>
    </joint>

    <!-- Finger 2 knuckle link -->
    <link name="${prefix}gripper_finger2_knuckle_link">
      <visual>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/crank_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/crank_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0222871"/>
        <origin xyz="0.0 0.0 0.0"/>
        <inertia ixx="8.72836e-06" ixy="7.0843e-15" ixz="-2.18833e-06"
                 iyy="7.26157e-06" iyz="7.6579e-15"
                 izz="3.3425e-06"/>
      </inertial>
    </link>

    <!-- Finger 1 finger joint -->
    <joint name="${prefix}gripper_finger1_finger_joint" type="continuous">
      <parent link="${prefix}gripper_finger1_knuckle_link"/>
      <child link="${prefix}gripper_finger1_finger_link"/>
      <axis xyz="0 -1 0"/>
      <origin xyz="0.0292080077784 0.0 -0.0227132622407" rpy="0 0 0"/>
      <limit lower="-3.14" upper="3.14" velocity="100.0" effort="0.1"/>
      <mimic joint="${prefix}gripper_finger1_joint" multiplier="0.4563942" offset="0"/>
    </joint>

    <!-- Finger 1 finger link -->
    <link name="${prefix}gripper_finger1_finger_link">
      <visual>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/rod_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/rod_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0222871"/>
        <origin xyz="0.0 0.0 0.0"/>
        <inertia ixx="8.72836e-06" ixy="7.0843e-15" ixz="-2.18833e-06"
                 iyy="7.26157e-06" iyz="7.6579e-15"
                 izz="3.3425e-06"/>
      </inertial>
    </link>

    <!-- Finger 2 finger joint -->
    <joint name="${prefix}gripper_finger2_finger_joint" type="continuous">
      <parent link="${prefix}gripper_finger2_knuckle_link"/>
      <child link="${prefix}gripper_finger2_finger_link"/>
      <axis xyz="0 -1 0"/>
      <origin xyz="0.0292080077784 0.0 -0.0227132622407" rpy="0 0 0"/>
      <limit lower="-3.14" upper="3.14" velocity="100.0" effort="0.1"/>
      <mimic joint="${prefix}gripper_finger1_joint" multiplier="0.4563942" offset="0"/>
    </joint>

    <!-- Finger 2 finger link -->
    <link name="${prefix}gripper_finger2_finger_link">
      <visual>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/rod_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/rod_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0222871"/>
        <origin xyz="0.0 0.0 0.0"/>
        <inertia ixx="8.72836e-06" ixy="7.0843e-15" ixz="-2.18833e-06"
                 iyy="7.26157e-06" iyz="7.6579e-15"
                 izz="3.3425e-06"/>
      </inertial>
    </link>

    <!-- Finger 1 inner knuckle joint -->
    <joint name="${prefix}gripper_finger1_inner_knuckle_joint" type="continuous">
      <parent link="${prefix}gripper_base_link"/>
      <child link="${prefix}gripper_finger1_inner_knuckle_link"/>
      <axis xyz="0 -1 0"/>
      <origin xyz="-0.016 0 0.10586" rpy="0 0 ${pi}"/>
      <limit lower="-3.14" upper="3.14" velocity="100.0" effort="0.1"/>
      <mimic joint="${prefix}gripper_finger1_joint" multiplier="1.49462955" offset="0"/>
    </joint>

    <!-- Finger 1 inner knuckle link -->
    <link name="${prefix}gripper_finger1_inner_knuckle_link">
      <visual>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/proximal_phalanx_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/proximal_phalanx_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0222871"/>
        <origin xyz="0.0 0.0 0.0"/>
        <inertia ixx="8.72836e-06" ixy="7.0843e-15" ixz="-2.18833e-06"
                 iyy="7.26157e-06" iyz="7.6579e-15"
                 izz="3.3425e-06"/>
      </inertial>
    </link>

    <!-- Finger 2 inner knuckle joint -->
    <joint name="${prefix}gripper_finger2_inner_knuckle_joint" type="continuous">
      <parent link="${prefix}gripper_base_link"/>
      <child link="${prefix}gripper_finger2_inner_knuckle_link"/>
      <axis xyz="0 -1 0"/>
      <origin xyz="0.016 0 0.10586" rpy="0 0 0"/>
      <limit lower="-3.14" upper="3.14" velocity="100.0" effort="0.1"/>
      <mimic joint="${prefix}gripper_finger1_joint" multiplier="1.49462955" offset="0"/>
    </joint>

    <!-- Finger 2 inner knuckle link -->
    <link name="${prefix}gripper_finger2_inner_knuckle_link">
      <visual>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/proximal_phalanx_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/proximal_phalanx_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0222871"/>
        <origin xyz="0.0 0.0 0.0"/>
        <inertia ixx="8.72836e-06" ixy="7.0843e-15" ixz="-2.18833e-06"
                 iyy="7.26157e-06" iyz="7.6579e-15"
                 izz="3.3425e-06"/>
      </inertial>
    </link>

    <!-- Finger 1 finger tip joint -->
    <joint name="${prefix}gripper_finger1_finger_tip_joint" type="continuous">
      <parent link="${prefix}gripper_finger1_inner_knuckle_link"/>
      <child link="${prefix}gripper_finger1_finger_tip_link"/>
      <axis xyz="0 1 0"/>
      <origin xyz="0.0394969463769 0.0 0.0382752038125" rpy="0 0 0"/>
      <limit lower="-3.14" upper="3.14" velocity="100.0" effort="0.1"/>
      <mimic joint="${prefix}gripper_finger1_joint" multiplier="1.49462955" offset="0"/>
    </joint>

    <!-- Finger 1 finger tip link -->
    <link name="${prefix}gripper_finger1_finger_tip_link">
      <visual>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/distal_phalanx_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/distal_phalanx_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0222871"/>
        <origin xyz="0.0 0.0 0.0"/>
        <inertia ixx="8.72836e-06" ixy="7.0843e-15" ixz="-2.18833e-06"
                 iyy="7.26157e-06" iyz="7.6579e-15"
                 izz="3.3425e-06"/>
      </inertial>
    </link>

    <!-- Finger 2 finger tip joint -->
    <joint name="${prefix}gripper_finger2_finger_tip_joint" type="continuous">
      <parent link="${prefix}gripper_finger2_inner_knuckle_link"/>
      <child link="${prefix}gripper_finger2_finger_tip_link"/>
      <axis xyz="0 1 0"/>
      <origin xyz="0.0394969463769 0.0 0.0382752038125" rpy="0 0 0"/>
      <limit lower="-3.14" upper="3.14" velocity="100.0" effort="0.1"/>
      <mimic joint="${prefix}gripper_finger1_joint" multiplier="1.49462955" offset="0"/>
    </joint>

    <!-- Finger 2 finger tip link -->
    <link name="${prefix}gripper_finger2_finger_tip_link">
      <visual>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/distal_phalanx_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://dh_ag95_gripper/meshes/visual/distal_phalanx_Link.STL" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.0222871"/>
        <origin xyz="0.0 0.0 0.0"/>
        <inertia ixx="8.72836e-06" ixy="7.0843e-15" ixz="-2.18833e-06"
                 iyy="7.26157e-06" iyz="7.6579e-15"
                 izz="3.3425e-06"/>
      </inertial>
    </link>

    <!-- Include transmissions -->
    <xacro:dh_ag95_gripper_transmission prefix="${prefix}"/>

  </xacro:macro>

</robot> 